    "react-router-dom": "^5.0.0",
    "react-scripts-ts": "3.1.0",
    "sass": "^1.20.1"
  },
  "scripts": {
    "build:css": "node-sass src/ -o src/ ",
    "watch:css": "npm run build:css && node-sass src/ -o src/ -w -r",
